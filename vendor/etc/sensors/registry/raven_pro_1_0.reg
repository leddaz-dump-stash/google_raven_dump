#
# Raven sensor device configuration registry script for the Proto 1.0 stage.
#

# CDT information <0xPPPPSJIV>
# Filter value - <CDT>:<mask>
?+0x22100:0XFFFFFFF0

# LSM6DSR.
+/dev/lsm6dsr/0
bus_name=i3c0
bus_freq_hz=12500000
bus_addr=0x6a
i3c_addr=0xa
intr_id=45
trigger_type=2
fifo_enabled=1
md_threshold_m_per_s_sq=1.2264
md_window_ms=20
sd_window_ms=5000
fifo_max_odr=4000
fifo_max_wm=256
fifo_min_wm=1
gpio_id=gpio88

# LSM6DSR accel.
+/dev/lsm6dsr/0/accel
orientation_transform=2 -1 3

# LSM6DSR gyro.
+/dev/lsm6dsr/0/gyro
orientation_transform=2 -1 3

# LSM6DSR temp.
+/dev/lsm6dsr/0/temp

# ICP10101.
+/dev/icp10101/0
bus_name=i2c0
bus_freq_hz=400000
bus_addr=0x63

# ICP10101 baro.
+/dev/icp10101/0/baro

# VD6282.
+/dev/vd6282/0
bus_name=i2c0
bus_freq_hz=400000
bus_addr=0x20
pdm_id=4
pcm_rate_hz=24000

# VD6282 Spectral sensor
+/dev/vd6282/0/spectral
auto_gain=1

# VD6282 rear light sensor.
+/dev/vd6282/0/rls
lux_scale=58.823529
fix_fac_cal=1

# VD6282 flicker sensor.
+/dev/vd6282/0/flicker
auto_gain=1

# Camera VSYNC.
# 0 = rear telephoto
+/dev/camera_vsync/0
intr_id=56
intr_trigger_type=2
# 1 = rear ultra-wide-angle
+/dev/camera_vsync/1
intr_id=57
intr_trigger_type=2
# 2 = front
+/dev/camera_vsync/2
intr_id=58
intr_trigger_type=2
# 3 = rear
+/dev/camera_vsync/3
intr_id=59
intr_trigger_type=2

# TMD3719.
+/dev/tmd3719/0
bus_name=i2c0
bus_freq_hz=400000
bus_addr=0x39
intr_id=15
sync_delay_ns=0
# falling edge = 0, rising edge = 1
te2_alignment=1
anti_aliasing=1

# TMD3719 light sensor
+/dev/tmd3719/0/als
lux_scale=1.0
c_coef=1
r_coef=-0.765091
g_coef=0.986515
b_coef=-1.221424
itime_table_brightness=95 181 271 361 2048
itime_table=1999240 1562500 1106780 651050 181750
# Below itime tables are deprecated.
itime_table_30hz=1999240 1562500 1106780 651050 181750
itime_table_60hz=1999240 1562500 1106780 651050 181750
itime_table_90hz=1999240 1562500 1106780 651050 181750
itime_table_120hz=1999240 1562500 1106780 651050 181750
max_gain_table=11 10 10 10 9
fast_filter_size=10
slow_filter_size=100
aod_fast_filter_size=2
aod_slow_filter_size=2
#high_IR_threshold=0.68 # todo(b/194047112)
max_lux=250000

clear_gain10_ratio=2.0
red_gain10_ratio=2.0
green_gain10_ratio=2.0
blue_gain10_ratio=2.0

clear_gain9_ratio=4.0
red_gain9_ratio=4.0
green_gain9_ratio=4.0
blue_gain9_ratio=4.0

clear_gain8_ratio=8.0
red_gain8_ratio=8.0
green_gain8_ratio=8.0
blue_gain8_ratio=8.0

# TMD3719 proximity sensor.
+/dev/tmd3719/0/prox
auto_baseline=1
burst_delay_normal=160
burst_delay_120hz=220
burst_delay_aod=0
hxtalk=1
pulse_count=1
vcsel_ma=10
pd0_far_threshold=100
pd0_near_threshold=100
pd1_far_threshold=80
pd1_near_threshold=120
# "Low power" operating parameters.
pulse_len_us=128
filter_len=12
skip_count=0
pdata0_scale=6.25
pdata1_scale=4.76
gain0=1
gain1=2
# "High power" operating parameters.
hp_pulse_len_us=512
hp_filter_len=1
hp_skip_count=0
hp_pdata0_scale=1.55
hp_pdata1_scale=1.14
hp_pdata0_offset=0
hp_pdata1_offset=13
hp_gain0=1
hp_gain1=2

# Display info.
+/dev/display_info/0
# Link to ALS attached to this display.
attached_alsp=/dev/tmd3719/0

# For MagCC
+/dev/magcc/0
battery_status=/sys/class/power_supply/battery/status
usb_status=/sys/class/power_supply/usb/online
usb_current=/sys/class/power_supply/usb/current_now
usb_voltage=/sys/class/power_supply/usb/voltage_now
wireless_status=/sys/class/power_supply/wireless/online
wireless_current=/sys/class/power_supply/wireless/current_now
wireless_voltage=/sys/class/power_supply/wireless/voltage_now

# MMC56X3X.
+/dev/mmc56x3x/0
bus_name=i3c0
bus_freq_hz=12500000
bus_addr=0x30
i3c_addr=0x14

# MMC56X3X mag.
+/dev/mmc56x3x/0/mag
orientation_transform=-2 1 3
fac_cal_transform=0.9819 -0.0142 -0.0308 -0.1136 0.8782 -0.0439 0.0348 0.0310 1.0834 0.0 0.0 0.0

# s6sy79x touch sensor.
+/dev/s6sy79x/0
bus_name=spi0
bus_freq_hz=10000000
spi_mode=3
spi_word_size=8
intr_id=17
intr_type=3
intr_gpio_id=gpio40
# 1 = double tap
# 2 = single tap
# 3 = long press
# 4 = single tap and long press
gesture_type=4
single_tap_recess_size=136
single_tap_min_frame_count=2
single_tap_max_frame_count=26
single_tap_motion_tolerance=150
single_tap_max_touch_size_x=540
single_tap_max_touch_size_y=550
long_press_centroid_x=720
long_press_centroid_y=2361
long_press_distance_x=142
long_press_distance_y=142
long_press_min_frame_count=3
mm2px=20

# VSC.
+/vsc
single_tap_enable=1
long_press_enable=1
quick_pick_up_enable=1

# Record reg_script.
+/
reg_script=raven_pro_1_0.reg

# TBN.
+/tbn
tbn_mode=1
ap2aoc_intr_id=16
ap2aoc_intr_type=4
ap2aoc_gpio_id=gpio39
aoc2ap_gpio_id=gpio90

+/vsc/auto_brightness
# float[3]={LuxToEnter, LuxToExit, RemappedLux}
lux_remap=-1.0 -1.0 -1.0

# float[3]={MinDelta, MaxDelta, LuxRatio}
lux_debounce_up=1 200 0.1
lux_debounce_down=1 200 0.1
